<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">
    <ImageView android:src="@drawable/icon" android:layout_height="250dp" android:layout_width="250dp" android:layout_gravity="center" android:id="@+id/ivReturnedPic"></ImageView>
	<ImageButton android:id="@+id/ibTakePic" android:src="@drawable/icon"
	android:layout_gravity="center"
		android:layout_height="wrap_content" android:layout_width="125dp"></ImageButton>
	<Button android:text="Set Wallpaper" android:id="@+id/bSetWall"
	android:layout_gravity="center"
		android:layout_width="125dp" android:layout_height="wrap_content"></Button>

</LinearLayout>
